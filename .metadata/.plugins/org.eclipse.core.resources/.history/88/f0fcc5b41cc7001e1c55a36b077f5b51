import java.util.Locale;
import java.util.Scanner;

public class Main7 {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		double C = sc.nextDouble();
		char v;
		double F = ( 9 * C ) / 5 + 32;
		do {
			System.out.println(F);
			v = sc.next().charAt(0);
		}while (v != 'n' || v != 'N');
	}

}
