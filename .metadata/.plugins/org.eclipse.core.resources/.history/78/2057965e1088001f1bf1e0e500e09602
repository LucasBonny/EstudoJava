package application;

import java.util.ArrayList;
import java.util.List;

import shapes.Circle;
import shapes.Shape;

public class Program {

	public static void main(String[] args) {
		List<Shape> list = new ArrayList<>();
		
		list.add(new Circle());

	}
	public static double max(List<Shape> list) {
		double max = 0;
		for(Shape s : list) {
			max += s.area();
		}
		return max;
	}

}
